class SwitchTest 
{
	public static void main(String[] args) 
	{
		int num = 2;

		switch(num){
			case 1 : System.out.println("1번 선택함");
					 break;
			default: System.out.println("기타 선택함");
			case 2 : System.out.println("2번 선택함");
					 break;
			case 3 : System.out.println("3번 선택함");
					 break;

		}//switch end

	}
}
