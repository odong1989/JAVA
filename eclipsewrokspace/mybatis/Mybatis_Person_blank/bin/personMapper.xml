<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!-- 회원정보 처리 SQL -->
<mapper namespace="test.dao.PersonMapper">

<!-- 회원 정보 저장 -->
<insert id="insertPerson" parameterType="person">
insert into person (
	num
	, name
	, age
)
values (
	person_seq.nextval
	, #{name}
	, #{age}
)
</insert>



</mapper>
